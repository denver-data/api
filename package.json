   "license": "MIT",
   "dependencies": {
      "@google/maps": "^0.5.5",
      "@types/csv-parse": "^1.2.2",
      "@types/google__maps": "^0.5.6",
      "@types/graphql": "^14.2.3",
      "@types/lodash": "^4.14.137",
      "@types/node": "^12.7.2",
      "apollo-server": "^2.14.2",
      "axios": "^0.19.0",
      "csv-parse": "^4.4.5",
      "dotenv": "^8.1.0",
      "graphql": "^14.4.2",
      "lodash": "^4.17.15",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.10",
      "ts-node": "^8.3.0",
      "type-graphql": "^0.17.4",
      "typeorm": "^0.2.18",
      "typescript": "^3.5.3"
   },
   "scripts": {
      "start": "npm run db:migrate && ts-node src/api/index.ts",
